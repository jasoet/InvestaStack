    <groupId>com.secondstack</groupId>
    <artifactId>investastack</artifactId>
    <packaging>pom</packaging>
    <version>0.1</version>
    <name>Investa Stack</name>
    <inceptionYear>2012</inceptionYear>

    <modules>
        <module>investastack-repository</module>
    </modules>
